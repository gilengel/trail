{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": ["src/main.ts"],
  "project": ["src/**/*.ts"],
  "ignore": [
    "**/*.spec.ts",
    "**/*.test.ts"
  ],
  "ignoreBinaries": [
    "nest",
    "prettier",
    "jscpd",
    "knip",
    "prisma",
    "eslint",
    "tsc",
    "ts-node",
    "openapi-diff",
    "typedoc",
    "jest",
    "stryker",
    "openapi-cli",
    "commitlint"
  ],
  "ignoreDependencies": [
    "@nestjs/cli",
    "@stryker-mutator/jest-runner",
    "@types/multer",
    "commitlint",
    "conventional-changelog-conventionalcommits",
    "globals",
    "prisma",
    "stryker",
    "prettier",
    "typedoc",
    "eslint*",
    "jest",
    "ts-jest",
    "@types/jest",
    "@nestjs/testing",
    "openapi-cli",
    "openapi-diff",
    "source-map-support",
    "swagger-ui-dist",
    "ts-loader",
    "jest-mock-extended",
    "@compodoc/compodoc",
    "@golevelup/ts-jest",
    "@nestjs/common",
    "@nestjs/core",
    "@nestjs/platform-express",
    "@nestjs/serve-static",
    "@nestjs/swagger",
    "@prisma/client",
    "async",
    "class-transformer",
    "class-validator",
    "exifreader",
    "fast-xml-parser",
    "jest-extended",
    "multer",
    "prisma-class-validator-generator",
    "reflect-metadata",
    "rxjs",
    "uuid"
  ]
}