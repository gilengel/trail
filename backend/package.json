{
  "name": "@trail/backend",
  "version": "0.0.1",
  "description": "",
  "author": "Gil Engel",
  "private": true,
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "build": "nest build",
    "build:check": "test -f dist/main.js",
    "sourcemaps:check": "ls dist/**/*.map",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check  \"src/**/*.ts\"",
    "code:duplicates": "jscpd --pattern \"src/**/*.ts\" --threshold 3",
    "code:knip": "knip --strict",
    "code:typecheck": "tsc --noEmit",
    "code:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:migrate:init": "npx prisma migrate dev --name init && npm run start:dev",
    "start:migrate:dev": "prisma migrate deploy && npm run start:dev",
    "deps:audit": "npm audit --audit-level=high",
    "deps:check": "npm outdated",
    "swagger:generate": "ts-node src/swagger.generation.ts",
    "swagger:diff": "openapi-diff openapi.yaml dist/openapi.json",
    "doc": "typedoc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "NODE_ENV=test jest --config ./jest-unit.json",
    "test:unit:cov": "NODE_ENV=test jest --config ./jest-unit.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest-e2e.json",
    "test:e2e:cov": "jest --config ./jest-e2e.json --coverage",
    "test:mutation": "stryker run",
    "prisma:validate": "prisma validate",
    "prisma:check": "prisma migrate diff --from-schema-datamodel prisma/schema.prisma --to-schema-datasource prisma/schema.prisma --exit-code",
    "openapi:validate": "openapi-cli validate openapi.yaml",
    "commitlint": "commitlint --from=HEAD~1",
    "smoke": "node dist/index.js",
    "ci": "npm run format:check && npm run code:lint && npm run code:duplicates && npm run code:knip && npm run code:typecheck && npm run test:unit:cov && npm run test:e2e:cov"
  },
  "dependencies": {
    "@compodoc/compodoc": "^1.1.32",
    "@golevelup/ts-jest": "^1.1.1",
    "@nestjs/common": "^11.1.10",
    "@nestjs/core": "^11.1.10",
    "@nestjs/platform-express": "^11.1.10",
    "@nestjs/serve-static": "^5.0.4",
    "@nestjs/swagger": "^11.2.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "async": "^3.2.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "exifreader": "^4.33.1",
    "fast-xml-parser": "^5.3.3",
    "jest-extended": "^7.0.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "prisma-class-validator-generator": "^6.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@nestjs/cli": "^11.0.14",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.10",
    "@stryker-mutator/jest-runner": "^9.4.0",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "commitlint": "^20.2.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^62.0.0",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "knip": "^5.77.1",
    "openapi-cli": "^0.0.2",
    "openapi-diff": "^0.24.1",
    "prettier": "^3.7.4",
    "prisma": "^7.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "swagger-ui-dist": "5.31.0",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typedoc": "^0.28.15",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
