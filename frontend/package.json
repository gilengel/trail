  "scripts": {
    "build": "nuxt build",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "code:duplicates": "jscpd --gitignore --threshold 10",
    "code:knip": "knip --strict",
    "code:typecheck": "tsc --noEmit",
    "code:lint": "eslint . --fix",
    "deps:audit": "npm audit --audit-level=high",
    "deps:check": "npm outdated",
    "start:dev": "nuxt dev",
    "test": "vitest run",
    "test:browser:cov": "vitest run  --workspace=vitest.workspace.browser.ts --coverage",
    "test:unit:cov": "vitest run --coverage",
    "test:component:cov": "cypress run --config video=false --component",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type-check": "vue-tsc --noEmit",
    "ci": "npm run format:check && npm run code:lint && npm run code:duplicates && npm run code:knip && npm run code:typecheck && npm run test:unit:cov"
  },
