{
  "name": "trail-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:cov": "vitest run --coverage",
    "test:e2e": "playwright test",
    "cypress:run": "cypress run --config video=false --component",
    "test:component:cov": "cypress run --config video=false --component",
    "test:e2e:cov": "cypress run --config video=false --e2e",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/geojson": "^7946.0.14",
    "@types/leaflet": "^1.9.8",
    "axios": "^1.6.5",
    "cheerio": "^1.0.0-rc.12",
    "flag-icons": "^7.2.0",
    "pinia": "^2.1.7",
    "three": "^0.134.0",
    "troisjs": "^0.3.4",
    "uuid": "^9.0.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@bahmutov/cypress-code-coverage": "^2.6.1",
    "@cypress/code-coverage": "^3.12.29",
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^18.19.3",
    "@types/three": "^0.160.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-istanbul": "^1.2.1",
    "@vue-leaflet/vue-leaflet": "^0.10.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.0",
    "cypress": "^13.6.6",
    "cypress-vite": "^1.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "happy-dom": "^13.5.3",
    "jsdom": "^23.2.0",
    "leaflet": "^1.9.4",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "sass": "^1.70.0",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  }
}
